{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    // "jsx": "react-jsx",
    "skipLibCheck": true,
    "lib": ["esnext"],
    "types": ["react", "react-native"],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
